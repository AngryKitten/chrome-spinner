{
  "name": "Spinner",
  "description": "Makes EVERYTHING spin.",
  "version": "2.0",
  "action": {
    "default_title": "Spinner"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "spinner.js"
      ]
    }
  ],
  "manifest_version": 3
}